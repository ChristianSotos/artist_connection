<h2>Songs Pending Review</h2>
<table>
	<thead>
		<tr>
			<th><span></span></th>
			<th><span>Artist</span></th>
			<th><span>Song</span></th>
			<th><span>Genre</span></th>
			<th><span>Play Count</span></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
	<% @pending_songs.each{|song|%>
		<tr class='artist-row' id="<%=song.user.id%>">
			<td><button class='play-btn' id="<%=song.audio_file.url%>" data-alt-src='<%=song.id%>'>Play</button></td>
			<td><span><%=song.user.artist_name%></td>
			<td><span><%=song.name%></span></td>
			<td><span><%=song.genre.name%></span></td>
			<td><span><%=song.play_count%></span></td>
			<td><span><a href="/admin/review_song/<%=song.id%>">Add Review</a></span></td>
		</tr>
	<% } %>
	</tbody>
</table>


<h2>Songs Reviewed</h2>
<table>
	<thead>
		<tr>
			<th><span></span></th>
			<th><span>Artist</span></th>
			<th><span>Song</span></th>
			<th><span>Genre</span></th>
			<th><span>Play Count</span></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
	<% @reviewed_songs.each{|song|%>
		<tr class='artist-row' id="<%=song.user.id%>">
			<td><button class='play-btn' id="<%=song.audio_file.url%>" data-alt-src='<%=song.id%>'>Play</button></td>
			<td><span><%=song.user.artist_name%></td>
			<td><span><%=song.name%></span></td>
			<td><span><%=song.genre.name%></span></td>
			<td><span><%=song.play_count%></span></td>
			<td><span><a href="/admin/review_song/<%=song.id%>">View Review</a></span></td>
		</tr>
	<% } %>
	</tbody>
</table>