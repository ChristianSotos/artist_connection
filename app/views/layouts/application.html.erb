<!DOCTYPE html>
<html>
<head>
  <title>LabelResearch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id='popup'></div>
<div id='left-background'><%= image_tag 'courtlin-jabrae.jpg' %></div>
<div id='right-background'><%= image_tag 'roxxanne-montana.jpg' %></div>
<div id='container'>
	<%= render :partial => "partials/header" %>
	<%= render :partial => "partials/button_bar" %>
	<%= yield %>
	<div id='sidebar'></div>
	<%= render :partial => "partials/playbar" %>
</div>

</body>
</html>
